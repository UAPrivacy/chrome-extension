{
  "name": "UAPrivacy",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png",
    "default_title": "UAPrivacy"
  },
  "icons": {
    "128": "logo.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}