{
  "background": {
    "persistent": false,
    "scripts": [
      "background.bundle.js"
    ]
  },
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "popup.html",
    "default_title": "UAPrivacy"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "favicon.png"
  },
  "manifest_version": 2,
  "name": "UAPrivacy",
  "permissions": [
    "tabs",
    "https://*/"
  ]
}